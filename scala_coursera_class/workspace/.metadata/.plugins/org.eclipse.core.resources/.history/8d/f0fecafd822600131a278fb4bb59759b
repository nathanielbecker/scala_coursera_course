package week_1

object session2 {

def countChange(money: Int, coins: List[Int]): Int = {

//def countchangio(money: Int, coins: List[Int]), : Int = {
	if (money == 0) 1
	else if (money < 0) 0
	else if (coins.isEmpty) 0
	else for (i <- 1 to (math floor money/coins.head )) {
				countchangio(money - i*coins.head, coins.tail)
				}
	
	
	


}
}
}